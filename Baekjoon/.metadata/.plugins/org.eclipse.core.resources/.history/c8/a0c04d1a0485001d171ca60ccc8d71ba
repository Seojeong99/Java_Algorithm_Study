//					check=check/2;
//				}
//				else if(a[check]<b[i]) {
//					check=check+((n-check)/2);
//				}
//				if(check==0) {
//					if(a[0]==b[i]) {
//						System.out.println(1);
//						break;
//					}
//					else {
//						System.out.println(0);
//						break;
//					}
//				}
//				if(check>=n-1) {
//					if(check==n-1 && a[n-1]==b[i]) {
//						System.out.println(1);
//						break;
//					}
//					else {
//						System.out.println(0);
//						break;
//					}
//					
//				}
//				if(a[check]==b[i]) {
//					System.out.println(1);
//					break;
//				}
//			}
//		}
		
	}

}
